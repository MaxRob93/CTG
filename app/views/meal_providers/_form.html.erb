<%= simple_form_for(@meal_provider) do |f| %>
  <%= f.input :name %>
  <%= f.input :address%>
  <%= f.input :category %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, 'Create a Restaurant', class: 'btn btn-primary form-control' %>
<% end %>

